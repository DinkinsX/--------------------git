{
    "manifest_version": 2,
    "name": "Перевод git на русский",
    "version": "1.0.0",
    "content_scripts": [
      {
        "matches": [
          "https://vk.com/*", 
          "https://m.vk.com/*",
          "http://vk.com/*", 
          "http://m.vk.com/*"
        ],
        "js": ["jquery-3.6.0.min.js","content.js"]
      }
    ],
    "browser_action": {
      "default_icon": "icon.png"
    },
    "background": {
      "scripts": ["background.js"]
    }
  }